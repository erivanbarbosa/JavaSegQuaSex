
public class Calculadora {
	public static void main(String[] args ) {
		int numero1;
		int numero2;
		
		numero1 = 10;
		numero2 = 15;
		
		System.out.printf("A soma do numero 1 com o numero 2 é ", numero1 + numero2, "\n");
		System.out.println(numero1 - numero2);
		System.out.println(numero1 * numero2);
		System.out.println(numero1 / numero2);
	}
}
