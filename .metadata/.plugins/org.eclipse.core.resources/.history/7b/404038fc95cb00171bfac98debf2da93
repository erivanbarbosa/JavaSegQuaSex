
public class CaixaEletronico {
	public static void main(String[] args ) {
		System.out.println("Bem vindo ao caixa eletronico!");
		
		Conta conta1 = new Conta();
		conta1.setSaldo(1000.00);
		
		System.out.println("O seu saldo é igual a: " + conta1.verSaldo());
		
		double valorDeposito = 300.00;
		conta1.depositar(valorDeposito);
		
		System.out.println("Valor do deposito: " + valorDeposito);
		System.out.println("O seu saldo após o deposito é igual a: " + conta1.verSaldo());
		
		double valorSaque = 400.00;
		conta1.sacar(valorSaque);
		
		System.out.println("Valor do saque: " + valorSaque);
		System.out.println("O seu saldo após o saque é igual a: " + conta1.verSaldo());
		
		
		System.out.println();
		Conta conta2 = new Conta();
		conta2.setSaldo(1000.00);
		
		System.out.println("O seu saldo é igual a: " + conta2.verSaldo());
		
		valorDeposito = 300.00;
		conta2.depositar(valorDeposito);
		
		System.out.println("Valor do deposito: " + valorDeposito);
		System.out.println("O seu saldo após o deposito é igual a: " + conta2.verSaldo());
		
		valorSaque = 400.00;
		conta2.sacar(valorSaque);
		
		System.out.println("Valor do saque: " + valorSaque);
		System.out.println("O seu saldo após o saque é igual a: " + conta2.verSaldo());
		
		
	}
}
